{
		"version": 1,
		"disable_existing_loggers": false,
		"loggers": {
				"django.request": {
						"handlers": ["mail_admins"],
						"level": "ERROR"
				},
				"django": {
						"handlers": ["console"],
						"level": "INFO"
				}
		},
		"handlers": {
				"console": {
						"level": "DEBUG",
						"class": "logging.StreamHandler"
				},
				"mail_admins": {
						"level": "ERROR",
						"class": "django.utils.log.AdminEmailHandler",
						"include_html": true
				}
		}
}
